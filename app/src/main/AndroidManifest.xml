<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.praticelogin.bigkbear.timetablingonline" >

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Register"></activity>
        <activity android:name=".DisplayInfo"></activity>
        <activity android:name=".MainPage"></activity>
        <activity android:name=".AdminPage"></activity>
        <activity android:name=".AssignSubjectsToClasses"></activity>
        <activity android:name=".AssignTeachers"></activity>
        <activity android:name=".GeneratedTimetable"></activity>
        <activity android:name=".MasterFormTimetable"
            android:screenOrientation="landscape"></activity>
        <activity android:name=".MasterTeacherTimetable"
            android:screenOrientation="landscape"></activity>


    </application>

</manifest>
